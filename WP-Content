		<wp:post_password></wp:post_password>
		<wp:is_sticky>0</wp:is_sticky>
		<wp:postmeta>
			<wp:meta_key>_wp_old_slug</wp:meta_key>
			<wp:meta_value><![CDATA[principles-of-facilitation]]></wp:meta_value>
		</wp:postmeta>
		<wp:postmeta>
			<wp:meta_key>_sfwd-topic</wp:meta_key>
			<wp:meta_value><![CDATA[a:3:{s:17:"sfwd-topic_course";s:3:"192";s:17:"sfwd-topic_lesson";s:3:"717";s:29:"sfwd-topic_forced_lesson_time";s:0:"";}]]></wp:meta_value>
		</wp:postmeta>
