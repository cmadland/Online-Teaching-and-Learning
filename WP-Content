		<wp:post_date_gmt>2014-12-05 22:25:44</wp:post_date_gmt>
		<wp:comment_status>open</wp:comment_status>
		<wp:ping_status>open</wp:ping_status>
		<wp:post_name>facilitation-principles</wp:post_name>
		<wp:status>publish</wp:status>
